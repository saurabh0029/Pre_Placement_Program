    "from scipy import stats\n",
    "\n",
    "def perform_hypothesis_test(list1, list2):\n",
    "    t_statistic, p_value = stats.ttest_ind(list1, list2)\n",
    "    return p_value\n",
    "\n",
    "list1 = [5, 10, 15, 20, 25]\n",
    "list2 = [10, 20, 30, 40, 50]\n",
    "p_value = perform_hypothesis_test(list1, list2)\n",
    "print(\"P-value:\", p_value)"
   ]
